import javax.swing.JOptionPane;
import java.util.Locale;
public class Dialog {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US);
		String s = JOptionPane.showInputDialog(null, "Enter your height in meters");
		double h = Double.parseDouble(s);
		String s2 = JOptionPane.showInputDialog(null, "Enter your weight in kg");
		double w = Double.parseDouble(s2);
		double bmi = w / (h * h);
		JOptionPane.showMessageDialog(null, "Height = " + h + " \nWeight = " + w + " \n" + "Your BMI = " + bmi);

	}

}
