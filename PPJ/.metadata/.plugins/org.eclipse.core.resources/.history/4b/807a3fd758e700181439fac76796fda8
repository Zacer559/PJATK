
public class Reclter {

	public static int fiboR(int n) {
		if (n >= 0 && n < 2) {
			return n;
		} else
			return fiboR(n - 1) + fiboR(n - 2);
	}
	 public static int fiboI(int n) 
	 {	int a=0;
	 	int b=1;
	 	int buff;
		 for (int i=0; i<=n; i++)
		 {	
			 buff=b;
			 b=b+a;
			 a=buff;
			 
			 
			 
			 
		 }
		 
		 return a;
	 }
	
	
	
	
	public static void main(String[] args) {
		int n = 0;
		System.out.println(fiboR(n));
		System.out.println(fiboR(n));
	}

}
